<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logmyway"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17"
        />
    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />        

    <application
        android:allowBackup="true"
        android:icon="@drawable/logmyway"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.logmyway.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.logmyway.LogMyWayActivity"
            android:label="@string/app_name" >
         </activity>
         <activity
            android:name="com.example.logmyway.ChangePassword"
            android:label="@string/app_name" >
         </activity>
         
         <activity
            android:name="com.example.logmyway.RouteList"
            android:label="@string/app_name" >
         </activity>
         
         <activity
            android:name="com.example.logmyway.KidList"
            android:label="@string/app_name" >
         </activity>
         
         <activity
            android:name="com.example.logmyway.GPSActivity"
            android:label="@string/app_name" >
         </activity>
         
    </application>        

</manifest>
